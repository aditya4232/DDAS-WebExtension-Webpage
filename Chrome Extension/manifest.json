{
  "manifest_version": 3,
  "name": "Data Duplication Alert System",
  "version": "1.0",
  "description": "Alerts users when they try to download duplicate files.",
  "permissions": [
    "downloads",         
    "downloads.shelf",   
    "storage",           
    "notifications"      
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
