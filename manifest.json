{
    "manifest_version": 3,
    "name": "File Management System Extension",
    "version": "1.0",
    "permissions": ["downloads", "storage", "tabs"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "https://your-website-url.com/*"
    ]
  }
  